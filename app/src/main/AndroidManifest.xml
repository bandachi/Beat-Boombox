<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="matthew_eric_nick.hack_the_90s">

    <application
        android:allowBackup="true"
        android:icon="@drawable/walkman_base"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="matthew_eric_nick.hack_the_90s.GameActivity" />
        <activity android:name="matthew_eric_nick.hack_the_90s.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="matthew_eric_nick.hack_the_90s.GameOverLose" />
        <activity android:name="matthew_eric_nick.hack_the_90s.Story" />
        <activity android:name="matthew_eric_nick.hack_the_90s.Instructions"></activity>
    </application>

</manifest>